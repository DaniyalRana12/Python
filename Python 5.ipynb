   "source": [
    "#Prograam to Swap Variables\n",
    "x=int(input(\"Enter value of x: \\n\"))\n",
    "y=int(input(\"Enter value of y: \\n\"))\n",
    "temp = x\n",
    "x = y\n",
    "y = temp\n",
    "print(\"The value of x after swapping: \\n\",x) \n",
    "print(\"The value of y after swapping: \\n\",y)"
   ]
